<Project Sdk="Microsoft.NET.Sdk">
    
    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <AssemblyName>JNysteen.Range</AssemblyName>
        <RootNamespace>JNysteen.Range</RootNamespace>
        <Version>1.0.0</Version>
        <Authors>Jesper Nysteen</Authors>
        <Description>A library for simple ranges</Description>
        <IsPackable>true</IsPackable>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <DocumentationFile>bin\Debug\JNysteen.Range.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <DocumentationFile>bin\Release\JNysteen.Range.xml</DocumentationFile>
    </PropertyGroup>

</Project>
